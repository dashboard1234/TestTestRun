<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gan.testtestrun">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/reminder"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RetrieveActivity" />

        <provider
            android:name=".SignalProvider"
            android:authorities="com.example.gan.testtestrun.SignalProvider" />

        <activity android:name=".MonitorActivity" />
        <activity android:name=".BaseActivity"></activity>
        <activity android:name=".MyPreferencesActivity"></activity>
    </application>

</manifest>